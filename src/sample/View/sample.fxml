                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Button fx:id="browse_path" layoutX="835.0" layoutY="465.0" mnemonicParsing="false" onAction="#chooseResultPath" text="Browse">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
